  "bugs": {
    "url": "https://github.com/keitakn/javascript-fp/issues"
  },
  "homepage": "https://github.com/keitakn/javascript-fp#readme",
  "private": true,
  "devDependencies": {
    "@types/ejs": "^2.6.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.5",
    "jest": "^23.1.0",
    "prettier": "^1.13.5",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.4.1",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "express": "^4.16.3"
  }
