{
  "name": "javascript-fp",
  "version": "1.0.0",
  "description": "functional programming for JavaScript(TypeScript)",
  "scripts": {
    "test": "jest --config=jest.config.json",
    "test:coverage": "jest --config=jest.config.json --collectCoverage=true",
    "tslint": "tslint --project tsconfig.json --config tslint.json 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "format": "tslint --project tsconfig.json --config tslint.json --fix 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "build": "tsc",
    "build:watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keitakn/javascript-fp.git"
  },
  "author": "keitakn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keitakn/javascript-fp/issues"
  },
  "homepage": "https://github.com/keitakn/javascript-fp#readme",
  "private": true,
  "devDependencies": {
    "@types/jest": "^24.0.20",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.11.7",
    "@types/ramda": "^0.26.33",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.5.0",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "log4js": "^5.3.0",
    "ramda": "^0.26.1"
  }
}
